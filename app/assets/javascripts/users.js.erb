var cities = <%= Rails.cache.fetch("cities") { JSON.parse(File.read(Rails.root.join('app/assets/json/cities.json'))) } %>
var countries = <%= Rails.cache.fetch("countries") { JSON.parse(File.read(Rails.root.join('app/assets/json/countries.json'))) } %>

$(document).on('page:change', function() {
  if($('body.users_index').length > 0) {
    $( "#city" ).autocomplete({
     source: cities
    });
    
    $( "#country" ).autocomplete({
     source: countries
    });
  }
});